CREATE TABLE Rent_Status(

Rent_ID VARCHAR2(10) CONSTRAINT Rent_ID_PK PRIMARY KEY,

Rent_Date DATE,

Return_Date DATE ,

Vid_ID VARCHAR2(10),

CONSTRAINT Vid_ID_FK1 FOREIGN KEY (Vid_ID) REFERENCES Video(Vid_ID),

Mem_ID VARCHAR2(10),

CONSTRAINT Mem_ID_FK2 FOREIGN KEY (Mem_ID) REFERENCES

Member(Mem_ID));

